$('.infinite-scroll').append("<%=j render(partial:'product_status_table', collection: @products_nonfav, as: :product, locals: { show_product_link: true }) %>");

<% if @products_nonfav.current_page == @products_nonfav.total_pages %>
  $('#view-more').remove();
<% else %>
  $('#view-more a').attr('href', '<%= url_for(page: @products_nonfav.current_page + 1) %>');
<% end %>
