{
  "description": "Asthmatic 18-45 year old, possibly immunized for flu (0041)",
  <% sex=gender %>
  "first": "<%= forename(sex) %>",
  "last": "<%= surname %>",
  "gender": "<%= sex %>",
  "birthdate": <%= between(Time.gm(1970, 1, 1), Time.gm(2005, 12, 31)) %>,
